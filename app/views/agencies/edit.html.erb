<h1>Edit agency infos</h1>
<div class="container">
  <%= simple_form_for @agency do |f| %>
    <%= f.input :text, :input_html => { 'rows' => 5} %>
    <%= f.input :address %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.button :submit %>
  <% end %>
  <%= link_to 'Back to Dashboard', dashboard_path %>
</div>

